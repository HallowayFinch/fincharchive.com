    <link>{{ site.url | default: site.github.url }}</link>
    <description>{{ site.description | xml_escape }}</description>
    <lastBuildDate>{{ site.time | date_to_rfc822 }}</lastBuildDate>
    <generator>Jekyll</generator>

    {%- assign EMPTY = '' | split: '' -%}
    {%- assign logs  = site.logs           | default: EMPTY -%}
    {%- assign notes = site["field-notes"] | default: EMPTY -%}
    {%- assign items = EMPTY | concat: logs | concat: notes | sort:'date' | reverse -%}

    {%- for p in items limit: 20 -%}
    <item>
      <title>{{ p.title | xml_escape }}</title>
      <link>{{ p.url | absolute_url }}</link>
      <guid isPermaLink="true">{{ p.url | absolute_url }}</guid>
      <pubDate>{{ p.date | date_to_rfc822 }}</pubDate>
      <description>{{ p.excerpt | strip_html | strip_newlines | xml_escape }}</description>
      {%- assign img = p.og_image | default: p.hero_image -%}
      {%- if img -%}
      <enclosure url="{{ img | absolute_url }}" type="image/jpeg" />
      {%- endif -%}
    </item>
    {%- endfor -%}
  </channel>
