---
layout: null
permalink: /field-notes/feed.xml
---
<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0">
<channel>
  <title>{{ site.title }} â€” Field Notes</title>
  <link>{{ site.url }}/field-notes/</link>
  <description>{{ site.description }}</description>
  {% assign items = site['field-notes'] | sort: 'date' | reverse %}
  {% for item in items %}
    <item>
      <title>{{ item.title | xml_escape }}</title>
      <link>{{ item.url | absolute_url }}</link>
      <pubDate>{{ item.date | date_to_rfc822 }}</pubDate>
      <guid>{{ item.url | absolute_url }}</guid>
      <description><![CDATA[{{ item.content }}]]></description>
    </item>
  {% endfor %}
</channel>
</rss>
